package com.config.model;

import javax.validation.constraints.NotNull;
import javax.validation.constraints.Size;

public class BookIdentiry {
    @NotNull
    @Size(max = 20)
    private String bookId;

    @NotNull
    @Size(max = 20)
    private String libraryId;
    
    public BookIdentiry() {

    }

	public BookIdentiry(@NotNull @Size(max = 20) String bookId, @NotNull @Size(max = 20) String libraryId) {
		super();
		this.bookId = bookId;
		this.libraryId = libraryId;
	}
    
    
    
}
