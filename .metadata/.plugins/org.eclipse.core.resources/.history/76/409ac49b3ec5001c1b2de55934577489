package com.config.model;

import javax.persistence.EmbeddedId;
import javax.validation.constraints.NotNull;
import javax.validation.constraints.Size;


public class Book {
	
    @EmbeddedId
    private BookIdentiry bookIdentity;
    
    @NotNull
    @Size(max = 50)
	private String title;
    
    
	private int price;
}

